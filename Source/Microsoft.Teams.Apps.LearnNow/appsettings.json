{
  "MicrosoftAppId": "{{ Bot Id }}",
  "MicrosoftAppPassword": "{{ Bot Password }}",
  "Bot": {
    "AppBaseUri": "{{ Application base path }}",
    "SecurityKey": "",
    "TenantId": "",
    "CacheDurationInMinutes": 60,
    "CardCacheDurationInHour": 12
  },
  "Storage": {
    "TableStorageConnectionString": "{{ Storage Table connection string }}",
    "BlobConnectionString": "{{Blob connection string}}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "BingSearch": {
    "Endpoint": "{{ Bing Search API end point }}",
    "Key": "{{ Subscription key for Bing Search API service }}",
    "SafeSearch": "strict"
  },
  "SQLStorage": {
    "ConnectionString": ""
  },
  "SecurityGroup": {
    "TeacherSecurityGroupId": "{{Id of the teacher security group}}",
    "AdminSecurityGroupId": "{{Id of the admin security group}}",
    "ModeratorsGroupId": "{{Group id of Moderators team in Teams}}"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "",
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information"
    }
  },
  "i18n": {
    "DefaultCulture": "en-US",
    "SupportedCultures": "en-US"
  },
  "Cache": {
    "DurationInMinutes": 60
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "ApplicationIdURI": "{{ Application id URI }}",
    "ValidIssuers": "https://login.microsoftonline.com/TENANT_ID/v2.0,https://sts.windows.net/TENANT_ID/",
    "GraphScope": "openid profile offline_access https://graph.microsoft.com/Group.Read.All https://graph.microsoft.com/User.Read.All"
  },
  "UseKeyVault": true,
  "KeyVaultUrl": {
    "BaseURL": ""
  }
}